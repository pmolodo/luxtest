import os
import sys

import hou

from importlib import reload

THIS_HIP = os.path.abspath(hou.hipFile.path())
LUXTEST_ROOT = os.path.dirname(THIS_HIP)

if LUXTEST_ROOT not in sys.path:
    sys.path.append(LUXTEST_ROOT)

import luxtest_hou_utils
reload(luxtest_hou_utils)

luxtest_hou_utils.standardize_node_names(dry_run=True)
luxtest_hou_utils.standardize_output_names(dry_run=True)
luxtest_hou_utils.reset_net_box_summaries(dry_run=True)

from luxtest_hou_utils import *

import __main__
__main__.__dict__.update({key: val for key, val in globals().items() if not key.startswith("_")})

